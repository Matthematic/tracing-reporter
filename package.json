{
  "name": "tracing-reporter-js",
  "version": "1.0.0",
  "description": "Builds a tracing report from JsDoc-annotated tests",
  "main": "src/reporter.js",
  "private": false,
  "bin": {
    "tracing-reporter-js": "bin/tracing-reporter-js.js"
  },
  "keywords": [
    "Cerner",
    "tracing-reporter-js",
    "tracing-reporter",
    "tracing"
  ],
  "rollOut": {
    "groupId": "com.cerner.nursing.medication",
    "site": {
      "repositoryId": "cerner-release-main-site",
      "repositoryUrl": "http://repo.release.cerner.corp/nexus/service/local/repositories/main-site"
    }
  },
  "publishConfig": {
    "registry": "https://cernerrepos.net/api/npm/npm-integration-local"
  },
  "bugs": {
    "url": "http://jira2.cerner.com/browse/CCDEV"
  },
  "scripts": {
    "deploy-gh-pages": "npm run pack && gh-pages -d build",
    "lint": "eslint --ext .js src",
    "jest": "mocha test/**/*.test.js",
    "test": "npm run lint && npm run jest && npm run test:widio",
    "test:jest": "npm run jest",
    "test:lint": "npm run lint",
    "test:wdio": "wdio ./wdio.conf.js",
    "test:wdio:local": "tt-wdio --formFactors ['huge'] --locales ['en-US'] --browsers ['chrome'] --gridUrl='grid314.test.devcernerpowerchart.com'",
    "pack": "TERRA_DEV_SITE_PUBLIC_PATH='/medication-administration/tracing-reporter-js/' NODE_ENV=production webpack -p",
    "demo": "node demos/demo-runner.js",
    "start": "tt-serve",
    "clean:all": "rimraf ./package-lock.json && rimraf ./node_modules",
    "clean:install": "npm run clean:all && npm install",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "rimraf lib",
    "compile:build": "babel src --out-dir lib --copy-files",
    "test:pr": "git branch -D TEST-PR; git checkout -b TEST-PR && git push origin TEST-PR --force --no-verify && git checkout -",
    "kill:wdio": "PROC_ID=$(RESULT=$(sudo netstat -ltnp | grep :8080) && (echo $RESULT | awk '{print $7}' | awk -F/ '{print $1}')) && kill -9 $PROC_ID"
  },
  "repository": {
    "type": "git",
    "url": "https://github.cerner.com/medication-record/tracing-reporter-js.git"
  },
  "author": "Matthew Carr (Matthew.Carr2@cerner.com)",
  "license": "ISC",
  "dependencies": {
    "acorn": "^7.1.1",
    "framer-motion": "^2.6.5",
    "fs": "0.0.1-security",
    "glob": "^7.1.3",
    "https-proxy-agent": "^2.2.3",
    "jest": "^26.4.2",
    "jsdoc-api": "^5.0.1",
    "jsdoc-to-markdown": "^6.0.1",
    "minimist": "^0.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-table": "^7.5.0",
    "smoothscroll-polyfill": "^0.4.4",
    "terra-markdown": "^2.43.0",
    "underscore": "^1.9.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.11.5",
    "browserslist-config-terra": "^1.0.0",
    "core-js": "^3.3.2",
    "eslint": "6.5.0",
    "gh-pages": "^2.2.0",
    "mocha": "^5.2.0",
    "raf": "^3.4.1",
    "snap-shot-it": "^7.9.3",
    "terra-dev-site": "^6.2.0",
    "terra-toolkit": "^5.10.0",
    "wdio-spec-reporter": "^0.1.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  }
}
