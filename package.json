{
  "name": "tracing-reporter",
  "version": "1.0.1-alpha.2",
  "description": "Builds a tracing report from JsDoc-annotated tests",
  "main": "src/reporter.js",
  "bin": {
    "tracing-reporter": "bin/tracing-reporter.js"
  },
  "publishConfig": {
    "registry": "https://cernerrepos.net/api/npm/npm-integration-local"
  },
  "scripts": {
    "lint": "eslint --ext .js src",
    "test": "mocha test/**/*.test.js",
    "demo": "node demos/demo-runner.js",
    "release:patch": "fusion-make patch --ec-profile npm --no-pom",
    "release:minor": "fusion-make minor --ec-profile npm --no-pom",
    "release:major": "fusion-make major --ec-profile npm --no-pom",
    "release:auto": "fusion-make --auto --ec-profile npm --no-pom"
  },
  "repository": {
    "type": "git",
    "url": "https://github.cerner.com/medication-record/tracing-reporter.git"
  },
  "author": "Matthew Carr (Matthew.Carr2@cerner.com)",
  "license": "ISC",
  "dependencies": {
    "fs": "0.0.1-security",
    "glob": "^7.1.3",
    "jsdoc-api": "^5.0.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "6.5.0",
    "fusion-make": "^1.10.0",
    "mocha": "^5.2.0"
  }
}
